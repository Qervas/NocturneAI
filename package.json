{
  "name": "@nocturne/ai",
  "version": "0.1.0",
  "description": "Autonomous multi-agent system powered by GitHub Copilot with interactive terminal interface",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nocturne": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "dev:ui": "tsx src/cli.ts ui",
    "build": "tsup && npm run postbuild",
    "postbuild": "node -e \"const fs=require('fs');const path='dist/cli.js';let content=fs.readFileSync(path,'utf8');if(!content.startsWith('#!/usr/bin/env node')){fs.writeFileSync(path,'#!/usr/bin/env node\\n'+content);fs.chmodSync(path,0o755);}\"",
    "build:watch": "tsup --watch",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "copilot",
    "cli",
    "terminal",
    "autonomous",
    "multi-agent",
    "llm"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "diff": "^8.0.2",
    "dotenv": "^16.4.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "tiktoken": "^1.0.15",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/diff": "^7.0.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nocturne-ai.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nocturne-ai/issues"
  },
  "homepage": "https://github.com/yourusername/nocturne-ai#readme"
}
